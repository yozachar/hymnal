{
  "hymn_number": 6,
  "starts_on": "chorus",
  "chorus": [
    "അല്പകാലം മാത്രം",
    "ഈ ഭൂവിലെ വാസം",
    "സ്വര്‍പ്പൂരമാണെന്റെ നിത്യമാം വീട്‌",
    "എന്റെ നിത്യമാം വീട്‌"
  ],

  "stanzas": [
    {
      "stanza_number": 1,
      "lines": [
        "എന്‍ പ്രയാണകാലം നാലുവിരല്‍ നീളം",
        "ആയതിന്‍ പ്രതാപം കഷ്ടത മാത്രം",
        "ഞാന്‍ പറന്നു വേഗം പ്രിയനോടു ചേരും",
        "വിണ്‍മഹിമ പ്രാപിച്ചെന്നും",
        "വിശ്രമിച്ചീടും എന്നും വിശ്രമിച്ചീടും"
      ]
    },
    {
      "stanza_number": 2,
      "lines": [
        "പാളയത്തിനപ്പുറത്ത്‌ കഷ്ടമേല്‍ക്കുക നാം",
        "പാടുപെട്ട യേശുവിന്റെ നിന്ദ ചുമക്കാം",
        "നില്ക്കും നഗരം ഇല്ലിവിടെ",
        "പോര്‍ക്കളത്തിലത്രേ നാം",
        "നില്ക്ക വേണ്ട പോര്‍ പൊരുതു",
        "യാത്ര തുടരാം വേഗം യാത്ര തുടരാം"
      ]
    },
    {
      "stanza_number": 3,
      "lines": [
        "നാടുവിട്ടു വീടുവിട്ടു നാമധേയ കൂട്ടം വിട്ടു",
        "കാഠിന്യമാം ശോധനയില്‍ യാനം ചെയ്തോരായ്‌",
        "കൂടി ഒന്നായ്‌ വാഴാന്‍",
        "വാഞ്ഛിച്ച്രെത നാളായ്‌",
        "കാരുണ്യവാന്‍ പണികഴിച്ച",
        "കൊട്ടാരം തന്നില്‍ ആ കൊട്ടാരം തന്നില്‍"
      ]
    }
  ]
}
