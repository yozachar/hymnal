{
  "hymn_number": 9,
  "starts_on": "stanza-1",
  "chorus": [
    "ആരാധിക്കാം യേശുവിനെ",
    "യേശു ക്രിസ്തുവിനെ",
    "അധികാരമുള്ളവനേ",
    "വണങ്ങീടാം ദൈവകുഞ്ഞാടിനെ",
    "ആരിലുമുന്നതനേ"
  ],
  "stanzas": [
    {
      "stanza_number": 1,
      "lines": [
        "പുകഴ്ത്തീടാം യേശുവിനെ",
        "ക്രൂശിലെ ജയാളിയെ",
        "സ്തുതിച്ചീടാം യേശുവിനെ",
        "സ്തുതിക്കവന്‍ യോഗ്യനല്ലോ"
      ]
    },
    {
      "stanza_number": 2,
      "lines": [
        "വിശ്വസിക്കാം യേശുവിനെ",
        "ഏക രക്ഷകനേ",
        "ഏറ്റു പറയാം യേശുവിനെ",
        "കര്‍ത്താധി കര്‍ത്താവിനെ"
      ]
    },
    {
      "stanza_number": 3,
      "lines": [
        "സ്നേഹിച്ചിടാം യേശുവിനെ",
        "ഏറ്റം ര്രിയനായോനേ",
        "സേഡിച്ചിടാം യേശുവിനെ",
        "ഇന്നുമെന്നും അനന്യനെ"
      ]
    },
    {
      "stanza_number": 4,
      "lines": [
        "ഘോഷിച്ചിടാം യേശുവിനെ",
        "സത്യ സുവിശേഷത്തെ",
        "നോക്കി പാര്‍ക്കാം യേശുവിനെ",
        "വീണ്ടും വരുന്നവനേ"
      ]
    }
  ]
}
